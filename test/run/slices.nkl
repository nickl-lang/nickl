import std;

ar: [3]i64;

ar[0] = 4;
ar[1] = 5;
ar[2] = 6;

slice: []i64 = &ar;

i := 0;
while i < slice.size {
    endl: i8;                                  // TODO Cannot write ternary
    if i < slice.size - 1 { endl = " ".*[0]; } // TODO Cannot write char literal
    else { endl = "\n".*[0]; }                 // TODO Cannot write char literal
    std.printf("%li%c", slice[i], endl);
    i = i + 1;
}

