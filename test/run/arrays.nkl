import std;

ar: [4]i64;

ar[0] = 1;
ar[1] = 2;
ar[2] = 4;
ar[3] = 8;

i := 0;
while i < 4 {
    // @assert(ar[i] == 1 << i);
    std.printf("ar[%li]=%li", i, ar[i]);
    i = i + 1;
}

ar = [0, 1, 2, 3];

i = 0;
while i < 4 {
    // @assert(ar[i] == i);
    std.printf("ar[%li]=%li", i, ar[i]);
    i = i + 1;
}
