ar: array_t{i64, 4};

ar[0] = 1;
ar[1] = 2;
ar[2] = 4;
ar[3] = 8;

i := 0;
while i < 4 {
    __assert(ar[i] == 1 << i);
    __printf("ar[{}]={}\n", i, ar[i]);
    i = i + 1;
}

