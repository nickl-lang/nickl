Vec2 :: (T: type_t) -> type_t {
    import compiler;
    F :: compiler.StructField;
    return compiler.makeStruct(&([2]F){
        F{"x", T},
        F{"y", T},
    });
}

iv :: Vec2(i64){1, 2};
sv :: Vec2([]i8){"one", "two"};

import std;
std.printf("%li %li %s %s\n",
    iv.x, iv.y,
    sv.x.data, sv.y.data,
);
