vec2 :: (T: type_t) -> type_t {
    import compiler;
    F :: compiler.StructField;
    return compiler.makeStruct(&([2]F){
        F{"x", T},
        F{"y", T},
    });
}

ivec2 :: vec2(i64);
svec2 :: vec2([]i8);

iv :: ivec2{1, 2};
sv :: svec2{"one", "two"};

import std;
std.printf("%li %li %.*s %.*s\n",
    iv.x, iv.y,
    sv.x.size, sv.x.data,
    sv.y.size, sv.y.data,
);
