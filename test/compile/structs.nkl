main :: (argc: i32, argv: **i8) -> i32 {
    import libc;

    ivec2 :: struct {
        x: i64,
        y: i64,
    }

    v: ivec2;

    v.x = 4;
    v.y = 5;

    libc.printf("x=%li y=%li\n", v.x, v.y);

    return 0;
}

main(0, cast(**i8) u64{0});

import compiler;
builder := compiler.createBuilder();
compiler.build(builder, main, "structs");
compiler.freeBuilder(builder);
