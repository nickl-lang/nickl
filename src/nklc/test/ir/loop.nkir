extern "c" proc printf(:i64, ...) :i32

pub proc _entry() {

    alloc :u64 -> %i
    store :u64 0 -> %i
    jmp @loop

@loop
    load %i -> :u64 %_1
    cmp lt :u64 %_1, 5 -> %cond
    jmpz :i8 %cond, @endloop
    call printf, ("%zi\n", ..., :u64 %_1) -> :i32
    add :u64 %_1, 1 -> %_2
    store :u64 %_2 -> %i
    jmp @loop

@endloop
    ret

}

