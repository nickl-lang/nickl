(const printf () (link "c" (proc [(param () (ptr (const (i8)))) ("...")] (i32))))

(export (const main () (proc [] (i32) [
    (var data (i32) ())
    (call fillData [(addr (addr data))])
    (call printf ["data=%i\n" data])
    (return 0)
])))

(const fillData () (proc [(param p (ptr (ptr (i32))))] (void) [
    (assign (deref (deref p)) 42)
]))

(call main [])

