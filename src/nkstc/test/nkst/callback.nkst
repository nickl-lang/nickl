(const printf () (link "c" (proc [(param () (ptr (const (i8)))) ("...")] (i32))))

(export (const main () (proc [] (i32) [
    (var x () 42)
    (call printf ["%zi^2=%zi\n" x (call apply [x (proc [(param (x) (i64))] (i64) [
        (return (mul x x))
    ])])])
    (return 0)
])))

(const apply () (proc [(param x (i64)) (param f (proc[(param () (i64))] (i64)))] (i64) [
    (return (call f [x]))
]))

(call main [])

